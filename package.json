{
  "name": "@nsf-open/ember-utility-library-workspace",
  "author": "National Science Foundation",
  "license": "MIT",
  "private": true,
  "workspaces": [
    "packages/@nsf-open/*"
  ],
  "scripts": {
    "document": "lerna run --parallel --no-bail document",
    "test": "lerna run --stream --no-bail test -- --test-port=0",
    "lint:js": "lerna run --stream --parallel --no-bail lint:js",
    "cleanup": "lerna exec -- rm -rf ./dist ./coverage",
    "version": "lerna version --conventional-commits"
  },
  "devDependencies": {
    "lerna": "4.0.0"
  },
  "dependencies": {},
  "volta": {
    "node": "16.15.0",
    "npm": "8.8.0"
  }
}
